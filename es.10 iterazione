#include <stdio.h>

int main(int argc , char *argv[]) {
    int n;
    int somma=0;
    do{
        printf("inserisci n: ");
        scanf("%d",&n);
        
    }while(n<1);
    
    for(int i=0;i<=n;i++){
        somma = somma+i;
    }
    printf("Somma dei primi %d numeri: %d", n, somma);
    return 0;
}
