#include <stdio.h>

int main(int argc , char *argv[]) {
    int n1,n2;
    
    do{
        printf("inserisci n1: ");
        scanf("%d",&n1);
        printf("inserisci n2: ");
        scanf("%d",&n2);
    }while(n1<0 || n2<=n1 || n2<0);
    
    for(int i=n2-1; i>n1;i--){
        printf("%d \n",i);
    }
    
    return 0;
}
