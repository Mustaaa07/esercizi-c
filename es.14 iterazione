#include <stdio.h>

int main(int argc, char *argv[]) {
  
    int n1,n2,somma=0;
    
    do{
        printf("inserisci n1: ");
        scanf("%d",&n1);
        printf("inserisci n2: ");
        scanf("%d",&n2);
        
    }while((n1 && n2)<0 );
    
    for( int i =0 ;i<n2;i++){
        somma=somma+n1;
    }
    printf(" il prodotto e' %d \n ",somma);
    
    return 0;
}
